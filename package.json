{
  "name": "@yeep/client",
  "version": "1.1.0",
  "description": "Yeep JavaScript/Node.js API client",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yeepio/js-client.git"
  },
  "keywords": [
    "yeep",
    "client",
    "sdk"
  ],
  "author": {
    "name": "Dimitrios C. Michalakos"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yeepio/js-client/issues"
  },
  "homepage": "https://github.com/yeepio/js-client#readme",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest --config=jest.json",
    "coverage": "jest --config=jest.json --coverage",
    "lint": "eslint .",
    "prebuild": "rimraf ./dist",
    "build": "rollup -c",
    "dist": "cd ./dist && npm publish --access=public"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "detect-node": "^2.0.4",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.4",
    "jest-cli": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-es-module-interop": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-progress": "^1.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
